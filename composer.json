{
  "name": "bentools/doctrine-static",
  "type": "library",
  "description": "Doctrine Static - Mock managers and repositories in simple arrays, without any database.",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "doctrine/orm": "~2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "satooshi/php-coveralls": "^2.0",
    "squizlabs/php_codesniffer": "^3.2",
    "symfony/var-dumper": "~4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "BenTools\\DoctrineStatic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\DoctrineStatic\\Tests\\": "tests/"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
